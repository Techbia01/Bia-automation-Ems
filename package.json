{
  "name": "ems",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "test:report": "cypress run; npm run merge:reports; npm run generate:report",
    "test:full": "node scripts/run-tests-and-notify.js",
    "test:headed": "cypress run --headed",
    "test:headed:full": "cypress run --headed && npm run merge:reports && npm run generate:report && node scripts/send-to-slack.js",
    "merge:reports": "mochawesome-merge cypress/reports/*.json -o cypress/reports/report.json",
    "generate:report": "marge cypress/reports/report.json -f report -o cypress/reports",
    "send:slack": "node scripts/send-to-slack.js",
    "test:open": "cypress open",
    "test:login": "cypress run --spec 'cypress/e2e/Login/**/*.cy.js'",
    "test:login-open": "cypress open --e2e --browser chrome"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "cypress": "^14.5.3",
    "cypress-mochawesome-reporter": "^4.0.2",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.1.0",
    "mochawesome-report-generator": "^6.3.2"
  }
}
